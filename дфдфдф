elif shower_type == "Шторка":
        # === АРТИКУЛЫ ===
        sku_map = {
            "AAAAAAAA9" : (
                f"P10088{suffix}" if curtain_type == "Распашное" else
                f"P10049{suffix}" if curtain_type == "Гормошка" else "-"
            ),
