            "AAAAAAAA15": (
                f"J10255-19{suffix}" if handle_type == "Ручка полотенцесушитель" and frame_type == "Круглая труба" else
                f"J10265{suffix}"     if handle_type == "Ручка полотенцесушитель" and frame_type == "Квадратная труба" else
